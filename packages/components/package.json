{
  "name": "@teamnovu/kit-components",
  "version": "1.1.0",
  "description": "A collection of vue(3) components for jkweb",
  "type": "module",
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./Image": {
      "types": "./dist/Image.vue.d.ts",
      "import": "./dist/Image.mjs"
    },
    "./Dialog": {
      "types": "./dist/Dialog.vue.d.ts",
      "import": "./dist/Dialog.mjs"
    },
    "./Collapse": {
      "types": "./dist/Collapse.vue.d.ts",
      "import": "./dist/Collapse.mjs"
    },
    "./Accordion": {
      "types": "./dist/accordion/Accordion.vue.d.ts",
      "import": "./dist/Accordion.mjs"
    },
    "./AccordionItem": {
      "types": "./dist/accordion/AccordionItem.vue.d.ts",
      "import": "./dist/AccordionItem.mjs"
    },
    "./AccordionHeader": {
      "types": "./dist/accordion/AccordionHeader.vue.d.ts",
      "import": "./dist/AccordionHeader.mjs"
    },
    "./AccordionBody": {
      "types": "./dist/accordion/AccordionBody.vue.d.ts",
      "import": "./dist/AccordionBody.mjs"
    },
    "./HAccordion": {
      "types": "./dist/accordion/HAccordion.vue.d.ts",
      "import": "./dist/HAccordion.mjs"
    },
    "./HAccordionItem": {
      "types": "./dist/accordion/HAccordionItem.vue.d.ts",
      "import": "./dist/HAccordionItem.mjs"
    },
    "./AccessibleComponent": {
      "types": "./dist/AccessibleComponent.vue.d.ts",
      "import": "./dist/AccessibleComponent.mjs"
    }
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "scripts": {
    "build": "vite build",
    "watch": "NODE_ENV=development vite build --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:teamnovu/kit.git"
  },
  "keywords": [
    "vue",
    "vue3",
    "components"
  ],
  "author": "Elias Bernhaut",
  "homepage": "https://kit.novu.ch/packages/components/",
  "dependencies": {
    "@teamnovu/kit-composables": "workspace:*",
    "@vueuse/core": "^9.13.0",
    "lodash-es": "^4.17.21",
    "nanoid": "^5.0.6"
  },
  "devDependencies": {
    "@vueuse/shared": "^10.8.0"
  }
}
